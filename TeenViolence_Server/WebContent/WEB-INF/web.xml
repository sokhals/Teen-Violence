<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>TeenViolence_Server</display-name>
  <servlet>
    <servlet-name>AuthenticatingUser</servlet-name>
    <servlet-class>authentication.AuthenticatingUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ImageDataServlet</servlet-name>
    <servlet-class>imageData.ImageDataServlet</servlet-class>
  </servlet>


  <servlet>
    <servlet-name>FetchInstruction</servlet-name>
    <servlet-class>parameter.FetchInstruction</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>InitialParameter</servlet-name>
    <servlet-class>parameter.InitialParameter</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ParameterServlet</servlet-name>
    <servlet-class>parameter.ParameterServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Questionnaire</servlet-name>
    <servlet-class>questionnaire.Questionnaire</servlet-class>
  </servlet>


  <servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>registration.Register</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ImageFetcher</servlet-name>
    <servlet-class>imageData.ImageFetcher</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AuthenticatingUser</servlet-name>
    <url-pattern>/authentication/*</url-pattern>
  </servlet-mapping>

<servlet-mapping>
    <servlet-name>FetchInstruction</servlet-name>
    <url-pattern>/parameter/FetchInstruction/*</url-pattern>
  </servlet-mapping>

<servlet-mapping>
    <servlet-name>InitialParameter</servlet-name>
    <url-pattern>/parameter/InitialParameter/*</url-pattern>
</servlet-mapping>

<servlet-mapping>
    <servlet-name>ParameterServlet</servlet-name>
    <url-pattern>/parameter/ParameterServlet/*</url-pattern>
  </servlet-mapping>

<servlet-mapping>
    <servlet-name>Questionnaire</servlet-name>
    <url-pattern>/questionnaire/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/registration/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ImageDataServlet</servlet-name>
    <url-pattern>/imageData/ImageDataServlet/*</url-pattern>
  </servlet-mapping>
	
  <servlet-mapping>
    <servlet-name>ImageFetcher</servlet-name>
    <url-pattern>/imageData/ImageFetcher/*</url-pattern>
  </servlet-mapping>
</web-app>